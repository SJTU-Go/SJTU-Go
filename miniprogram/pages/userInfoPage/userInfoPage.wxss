  width:20%;
}
.verticaltool{  
  display: flex;
  flex-direction: column;
  text-align: left;}

.tool{
  display: flex;
  flex-direction: row;
  text-align: left-top;

}
.blocklleft
{display: flex;
  flex-direction: row;
  text-align: left;
  width:50%;
}
.blockright
{ 
  display: flex;
  flex-direction: row;
  text-align: center;
  align-items: center;
}

.block
{ margin-left: 100rpx;
  margin-top: 40rpx;
  height: 200rpx;
  width:100%;
  display: flex;
  align-items: center;
  justify-content: left;
}

.tool
{ height: 40%;
  display: flex;
  flex-direction: row;
  text-align: center;
  align-items: center;
}
.blockup
{display: flex;
  flex-direction: column;
  text-align: center;
  align-items: center;
  height:30%;}

