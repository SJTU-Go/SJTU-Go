/**index.wxss**/

.top {
  width: 100%;
  height: 130px;
  background: #23EBB9;
  padding-top: 15px;
  position: relative;
}

.center {
  width: 95%;
  height: 100px;
  background: white;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
  border-radius: 7px;
}
.center_img {
  width: 66px;
  height: 66px;
  border-radius: 50%;
  overflow: hidden;
}

.center_img image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.center_img .user_head {
  width: 100%;
  height: 100%;
}

.center_top {
  display: flex;
  flex-direction: row;
  width: 90%;
  height: 80px;
  margin: 0 auto;
  margin-top: 20rpx;
  border-bottom: 1px solid #EEEEEE;
}
.center_score {
  font-size: 13px;
  width: 33%;
  height: 35px;
  display: flex;
  flex-direction: row;
}
.center_score image {
  width: 25px;
  height: 25px;
}
.center_rank image {
  width: 25px;
  height: 25px;
}

.center_info {
  display: flex;
  flex-direction: column;
  margin-top: 10rpx;
  margin-left: 30px;
}

.center_name {
  font-size: 20px;
}
.title
{font-size: 32rpx;margin-top:40rpx ;margin-bottom:40rpx ;
  text-align: center;}
.center_phone {
  font-size: 14px;
  color: rgb(5, 163, 111);
}

.center_down {
  display: flex;
  flex-direction: row;
  width: 90%;
  height: 35px;
  margin: 0 auto;
  margin-top: 20rpx;
}

.center_rank {
  font-size: 13px;
  width: 33%;
  height: 35px;
  display: flex;
  flex-direction: row;
}

.rank_text {
  
  height: 35px;
  line-height: 35px;
  margin-left: 10rpx;
  color: #AAAAAA;
}

.gif-wave {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;

}
page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
